  "scripts": {
    "start": "export PORT=8000 && craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "serve": "craco build && serve -s build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "aws-deploy": "rm -rf ./build && npm run build && aws s3 sync ./build s3://gcf-frontend --profile=onepick && aws cloudfront create-invalidation --profile=onepick --distribution-id E2T32P31OV0P1O --paths /*",
    "aws-cache-invalidation": "aws cloudfront create-invalidation --profile=onepick --distribution-id E2T32P31OV0P1O --paths /*"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.0.0",
    "@storybook/addon-actions": "^6.4.17",
    "@storybook/addon-essentials": "^6.4.17",
